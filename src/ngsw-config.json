{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ],
      "urls": [
        "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "api-performance",
    "urls": [
      "/pokemon"
    ],
    "cacheConfig": {
      "maxSize": 200,
      "maxAge": "3d",
      "strategy": "performance"
    }
  }]
}